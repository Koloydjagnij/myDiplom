                    <th>Имя</th>
                    <th>Фамилия</th>
                    <th>Отчество</th>
                    <th>Город проживания</th>
                    <th>Социальный статус</th>
                    <th>Пол</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Enrollees.Family)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItems => item.IdFamilyTypeNavigation.NameFamilyType)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItems => item.IdParentNavigation.IdParentTypeNavigation.NameParentType)
                        </td>
                        <td>
